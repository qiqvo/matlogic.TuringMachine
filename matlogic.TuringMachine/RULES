q0 # -> q1 0 R
q1 1 -> q1 0 R
q1 0 -> q* 0
q1 a -> q1 1 R
q0 1 -> q2 0 R
q2 1 -> q2 1 R
q2 # -> q3 # R
q3 1 -> q4 0 R
q4 1 -> q4 1 R
q4 a -> q4 a R
q4 0 -> q5 a L
q5 1 -> q5 1 L
q5 a -> q5 a L
q5 0 -> q3 1 R
q3 a -> q6 a L
q6 1 -> q6 1 L
q6 # -> q6 # L
q6 0 -> q0 0 R
q3 0 -> q7 0 L
q7 # -> q7 0 L
q7 1 -> q7 0 L
q7 0 -> q* 0